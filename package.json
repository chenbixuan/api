{
  "name": "cbx",
  "version": "1.0.0",
  "description": "The egg ts example project that uses egg-sequelize plugin.",
  "private": true,
  "app": "cbx",
  "scripts": {
    "dev": "egg-bin dev",
    "test": "NODE_ENV=test npm run sequelize -- db:migrate && egg-bin test",
    "debug": "egg-bin debug",
    "debug-test": "npm run test-local",
    "start": "egg-scripts start --daemon --title=$npm_package_app --workers=1",
    "stop": "egg-scripts stop --title=$npm_package_app",
    "cov": "egg-bin cov",
    "ci": "npm run cov",
    "autod": "autod",
    "sequelize": "sequelize --",
    "prestart": "npm run cov && tsc"
  },
  "dependencies": {
    "egg": "^2.10.0",
    "egg-cors": "^2.2.1",
    "egg-scripts": "^2.11.1",
    "egg-sequelize": "^5.0.0",
    "lodash": "^4.17.15",
    "mysql2": "^1.6.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "autod": "^3.0.1",
    "egg-bin": "^4.8.1",
    "egg-mock": "^3.19.2",
    "factory-girl": "^5.0.2",
    "sequelize-cli": "^5.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "author": "colter",
  "license": "MIT"
}
